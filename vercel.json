{
  "buildCommand": null,
  "outputDirectory": ".",
  "framework": null,
  "headers": [
    {
      "source": "/(test|debug)(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "no-cache, no-store, must-revalidate" }
      ]
    }
  ],
  "rewrites": [
    { "source": "/test", "destination": "/test.html" },
    { "source": "/debug", "destination": "/debug.html" },
    { "source": "/((?!test\\.html|debug\\.html).*)", "destination": "/index.html" }
  ]
}
